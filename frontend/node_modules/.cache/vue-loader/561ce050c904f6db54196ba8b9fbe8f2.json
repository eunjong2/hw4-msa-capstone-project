tick\" />\n\n                    <v-btn\n                            style=\"postition:absolute; top:2%; right:2%\"\n                            @click=\"closeDialog()\"\n                            depressed\n                            icon \n                            absolute\n                    >\n                        <v-icon>mdi-close</v-icon>\n                    </v-btn>\n                </v-dialog>\n\n\n                <v-btn\n                        color=\"blue\" fab dark large\n                        style=\"position:fixed; bottom: 5%; right: 2%; z-index:99\"\n                        @click=\"openDialog=true;\"\n                >\n                    <v-icon>mdi-plus</v-icon>\n                </v-btn>\n            </div>\n        </v-col>\n        <v-row>\n            <Menu :offline=\"offline\" class=\"video-card\" v-for=\"(value, index) in values\" v-model=\"values[index]\" v-bind:key=\"index\" @delete=\"remove\"/>\n        </v-row>\n    </div>\n</template>\n\n<script>\n\n    const axios = require('axios').default;\n    import Menu from './../Menu.vue';\n\n    export default {\n        name: 'MenuManager',\n        components: {\n            Menu,\n        },\n        props: {\n            offline: Boolean\n        },\n        data: () => ({\n            values: [],\n            newValue: {},\n            tick : true,\n            openDialog : false,\n        }),\n        async created() {\n            var me = this;\n            if(me.offline){\n                if(!me.values) me.values = [];\n                return;\n            } \n\n            var temp = await axios.get(axios.fixUrl('/menus'))\n            me.values = temp.data._embedded.menus;\n            \n            me.newValue = {\n                'menuName': '',\n                'qty': 0,\n                'cookStatus': '',\n                'orderId': 0,\n                'orderStatus': '',\n            }\n        },\n        methods:{\n            closeDialog(){\n                this.openDialog = false\n            },\n            append(value){\n                this.tick = false\n                this.newValue = {}\n                this.values.push(value)\n                \n                this.$emit('input', this.values);\n\n                this.$nextTick(function(){\n                    this.tick=true\n                })\n            },\n            remove(value){\n                var where = -1;\n                for(var i=0; i<this.values.length; i++){\n                    if(this.values[i]._links.self.href == value._links.self.href){\n                        where = i;\n                        break;\n                    }\n                }\n\n                if(where > -1){\n                    this.values.splice(i, 1);\n                    this.$emit('input', this.values);\n                }\n            }\n        }\n    };\n</script>\n\n\n<style>\n    .video-card {\n        width:300px; \n        margin-left:4.5%; \n        margin-top:50px; \n        margin-bottom:50px;\n    }\n</style>\n\n"]}]}