xport default {\n        name: 'LargeObject',\n        components:{\n        },\n        data: () => ({\n            isFile: false,\n            type: this.value,\n        }),\n        props: {\n            value: [String, Number],\n            editMode: Boolean,\n            label: String\n        },\n        created() {\n            if(typeof this.value === 'string') {\n                this.type = 'text';\n            } else if(typeof this.value === 'number') {\n                this.type = 'number';\n            }\n        },\n        methods:{\n            change(){\n                this.$emit(\"input\", this.value);\n            }\n        }\n    }\n</script>\n"]}]}