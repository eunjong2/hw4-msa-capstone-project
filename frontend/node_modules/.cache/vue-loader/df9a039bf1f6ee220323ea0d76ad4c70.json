em-title>\n\n                        <v-list-item-subtitle style=\"font-size:25px; font-weight:700;\">\n                            [ Id :  {{data.id }} ] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                            [ MenuName :  {{data.menuName }} ] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                            [ Qty :  {{data.qty }} ] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                            [ CookStatus :  {{data.cookStatus }} ] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                            [ OrderId :  {{data.orderId }} ] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                            [ OrderStatus :  {{data.orderStatus }} ] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                        </v-list-item-subtitle>\n\n                    </v-list-item-content>\n                </v-list-item>\n\n                <v-divider v-if=\"n !== 6\" :key=\"`divider-${n}`\" inset></v-divider>\n            </template>\n        </v-list>\n\n        <v-col style=\"margin-bottom:40px;\">\n            <div class=\"text-center\">\n                <v-dialog\n                        v-model=\"openDialog\"\n                        width=\"332.5\"\n                        fullscreen\n                        hide-overlay\n                        transition=\"dialog-bottom-transition\"\n                >\n                    <template v-slot:activator=\"{ on, attrs }\">\n                        <v-fab-transition>\n                            <v-btn\n                                    color=\"blue\"\n                                    fab\n                                    dark\n                                    large\n                                    style=\"position:absolute; bottom: 5%; right: 2%; z-index:99\"\n                                    @click=\"openDialog=true;\"\n                            >\n                                <v-icon v-bind=\"attrs\" v-on=\"on\">mdi-plus</v-icon>\n                            </v-btn>\n                        </v-fab-transition>\n                    </template>\n\n                    <Menu :offline=\"offline\" class=\"video-card\" :isNew=\"true\" :editMode=\"true\" v-model=\"newValue\" @add=\"append\" v-if=\"tick\"/>\n                \n                    <v-btn\n                            style=\"postition:absolute; top:2%; right:2%\"\n                            @click=\"closeDialog()\"\n                            depressed \n                            icon \n                            absolute\n                    >\n                        <v-icon>mdi-close</v-icon>\n                    </v-btn>\n                </v-dialog>\n            </div>\n        </v-col>\n    </div>\n</template>\n\n<script>\n    const axios = require('axios').default;\n    import Menu from './../Menu.vue';\n\n    export default {\n        name: 'MenuManager',\n        components: {\n            Menu,\n        },\n        props: {\n            offline: Boolean,\n            editMode: Boolean,\n            isNew: Boolean\n        },\n        data: () => ({\n            values: [],\n            newValue: {},\n            tick : true,\n            openDialog : false,\n        }),\n        async created() {\n            if(this.offline){\n                if(!this.values) this.values = [];\n                return;\n            } \n\n            var temp = await axios.get(axios.fixUrl('/menus'))\n            temp.data._embedded.menus.map(obj => obj.id=obj._links.self.href.split(\"/\")[obj._links.self.href.split(\"/\").length - 1])\n            this.values = temp.data._embedded.menus;\n            \n            this.newValue = {\n                'menuName': '',\n                'qty': 0,\n                'cookStatus': '',\n                'orderId': 0,\n                'orderStatus': '',\n            }\n        },\n        methods: {\n            closeDialog(){\n                this.openDialog = false\n            },\n            append(value){\n                this.tick = false\n                this.newValue = {}\n                this.values.push(value)\n                \n                this.$emit('input', this.values);\n\n                this.$nextTick(function(){\n                    this.tick=true\n                })\n            }\n        },\n    };\n</script>\n\n\n<style>\n    .video-card {\n        width:300px; \n        margin-left:4.5%; \n        margin-top:50px; \n        margin-bottom:50px;\n    }\n</style>\n\n"]}]}